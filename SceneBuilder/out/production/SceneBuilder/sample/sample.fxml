<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<!--Make sure specifications of Pane can be utilized otherwise delete them in order to continue building
using Scene Builder-->

<GridPane alignment="center" stylesheets="@styles.css" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <HBox spacing="5.0" GridPane.rowIndex="1">
         <children>
            <Button mnemonicParsing="false" onAction="#handleAction" text="OK">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" text="Cancel">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" text="Help">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin></Button>
         </children>
         <GridPane.margin>
            <Insets bottom="5.0" />
         </GridPane.margin>
      </HBox>
      <Label fx:id="label" text="My Label" />
   </children>
   <padding>
      <Insets left="5.0" top="5.0" />
   </padding>
</GridPane>
